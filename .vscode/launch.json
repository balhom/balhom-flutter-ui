{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file", "app.env"
            ]
        },
        {
            "name": "web-html",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
                "--web-renderer", "html",
                "--web-port", "5000",
                "--dart-define-from-file", "app.env"
            ]
        },
        {
            "name": "web-canvaskit",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
                "--web-renderer", "canvaskit",
                "--web-port", "5000",
                "--dart-define-from-file", "app.env"
            ]
        }
    ]
}